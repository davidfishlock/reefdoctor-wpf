                                    </Grid.RowDefinitions>

                                    <TextBlock
                                        Margin="6"
                                        VerticalAlignment="Center"
                                        FontSize="18"
                                        Foreground="White"
                                        Text="Species Catalog" />

                                    <StackPanel
                                        Margin="6,6,6,6"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Center"
                                        Orientation="Horizontal">
                                        <Button
                                            Background="{StaticResource ReefDoctorGreenPrimary}"
                                            Command="{Binding ShowSpeciesBrowserCommand}"
                                            Content="View Species"
                                            Style="{StaticResource LauncherButton}" />
                                    </StackPanel>
                                </Grid>
                            </Grid>
                        </WrapPanel>
                    </Grid>
                </StackPanel>
            </Grid>
        </ScrollViewer>
        <VisualStateManager.VisualStateGroups>
            <VisualStateGroup Name="IntroStates">
                <VisualState Name="Intro">
                    <Storyboard>
                        <DoubleAnimation
                            BeginTime="0:0:0"
                            Storyboard.TargetName="FishPanel"
                            Storyboard.TargetProperty="(UIElement.Opacity)"
                            From="0"
                            To="1"
                            Duration="0:0:0.3" />
                        <DoubleAnimation
                            BeginTime="0:0:0"
                            Storyboard.TargetName="FishScale"
                            Storyboard.TargetProperty="ScaleX"
                            To="1"
                            Duration="0:0:0.3" />
                        <DoubleAnimation
                            BeginTime="0:0:0"
                            Storyboard.TargetName="FishScale"
                            Storyboard.TargetProperty="ScaleY"
                            To="1"
                            Duration="0:0:0.3" />
                        <DoubleAnimation
                            BeginTime="0:0:0"
                            Storyboard.TargetName="FishTranslate"
                            Storyboard.TargetProperty="Y"
                            To="0"
                            Duration="0:0:0.3" />
                        <DoubleAnimation
                            BeginTime="0:0:0.05"
                            Storyboard.TargetName="InvertPanel"
                            Storyboard.TargetProperty="(UIElement.Opacity)"
                            From="0"
                            To="1"
                            Duration="0:0:0.3" />
                        <DoubleAnimation
                            BeginTime="0:0:0.05"
                            Storyboard.TargetName="InvertScale"
                            Storyboard.TargetProperty="ScaleX"
                            To="1"
