                                        VerticalAlignment="Center"
                                        FontSize="18"
                                        Foreground="White"
                                        Text="Juvenile Fish" />

                                    <StackPanel
                                        Margin="6,6,6,6"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Center"
                                        Orientation="Horizontal">
                                        <Button
                                            Command="{Binding StartWorkshopCommand}"
                                            CommandParameter="JuvenileFish-Workshop"
                                            Content="Workshop"
                                            Style="{StaticResource LauncherButton}" />
                                        <Button
                                            Command="{Binding StartWorkshopCommand}"
                                            CommandParameter="JuvenileFish-Quiz"
                                            Content="Quiz"
                                            Style="{StaticResource LauncherButton}" />
                                        <!--<Button
                                            Command="{Binding StartWorkshopCommand}"
                                            CommandParameter="JuvenileFish-Exam"
                                            Content="Exam"
                                            Style="{StaticResource LauncherButton}"
                                            Visibility="{Binding IsExamsEnabled, Converter={StaticResource BooleanToVisibility}}" />-->
                                    </StackPanel>
                                </Grid>
                            </Grid>

                            <Grid
                                x:Name="SpeciesBrowserPanel"
                                Margin="0,0,12,12"
                                Background="{StaticResource MainPanelBg}"
                                CacheMode="BitmapCache"
                                Opacity="0"
                                RenderTransformOrigin="0.5,0.5">
                                <Grid.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform
                                            x:Name="SpeciesBrowserScale"
                                            ScaleX="0.98"
                                            ScaleY="0.98" />
                                        <TranslateTransform x:Name="SpeciesBrowserTranslate" Y="12" />
                                    </TransformGroup>
                                </Grid.RenderTransform>
                                <Border Height="196" HorizontalAlignment="Stretch">
                                    <Image
