<UserControl
    x:Class="ReefDoctorId.WPF.Controls.MainMenu"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="ControlRoot"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">

    <Grid x:Name="RootGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <Image
            Width="300"
            Margin="24,45,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Source="/Assets/Logo_White.png" />

        <ScrollViewer
            Grid.Row="1"
            Margin="0,18,6,0"
            PanningMode="VerticalFirst"
            Style="{StaticResource MetroScrollViewer}"
            VerticalScrollBarVisibility="Auto">
            <Grid Margin="0,0,0,0">
                <StackPanel HorizontalAlignment="Stretch" Orientation="Vertical">
                    <Grid Style="{StaticResource MainPagePanel}">
                        <WrapPanel
                            Margin="12,12,0,24"
                            ItemWidth="{Binding ActualWidth, RelativeSource={RelativeSource Self}, Converter={StaticResource FlexWrapPanel}}"
                            Orientation="Horizontal">

                            <Grid
                                x:Name="FishPanel"
                                Margin="0,0,12,12"
                                Background="{StaticResource MainPanelBg}"
                                CacheMode="BitmapCache"
                                Opacity="0"
                                RenderTransformOrigin="0.5,0.5">
                                <Grid.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform
                                            x:Name="FishScale"
                                            ScaleX="0.98"
                                            ScaleY="0.98" />
                                        <TranslateTransform x:Name="FishTranslate" Y="12" />
                                    </TransformGroup>
                                </Grid.RenderTransform>
                                <Border Height="194" HorizontalAlignment="Stretch">
                                    <Image
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Source="/Assets/Backgrounds/BluestreakCleanerWrasse.jpg"
                                        Stretch="UniformToFill" />
                                </Border>

                                <Grid VerticalAlignment="Bottom" Background="{StaticResource MainPanelBg}">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>

                                    <TextBlock
                                        Margin="6"
                                        VerticalAlignment="Center"
                                        FontSize="18"
                                        Foreground="White"
                                        Text="Fish" />

                                    <StackPanel
                                        Margin="6,6,6,6"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Center"
                                        Orientation="Horizontal">
                                        <Button
                                            Command="{Binding StartWorkshopCommand}"
                                            CommandParameter="Fish-Workshop"
                                            Content="Workshop"
                                            Style="{StaticResource LauncherButton}" />
                                        <Button
                                            Command="{Binding StartWorkshopCommand}"
                                            CommandParameter="Fish-Quiz"
                                            Content="Quiz"
                                            Style="{StaticResource LauncherButton}" />
                                        <Button
                                            Command="{Binding StartWorkshopCommand}"
                                            CommandParameter="Fish-Exam"
                                            Content="Exam"
                                            Style="{StaticResource LauncherButton}"
                                            Visibility="{Binding IsExamsEnabled, Converter={StaticResource BooleanToVisibility}}" />
                                    </StackPanel>

                                </Grid>
                            </Grid>

                            <Grid
                                x:Name="InvertPanel"
                                Margin="0,0,12,12"
                                Background="{StaticResource MainPanelBg}"
                                CacheMode="BitmapCache"
                                Opacity="0"
                                RenderTransformOrigin="0.5,0.5">
                                <Grid.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform
                                            x:Name="InvertScale"
                                            ScaleX="0.98"
                                            ScaleY="0.98" />
                                        <TranslateTransform x:Name="InvertTranslate" Y="12" />
                                    </TransformGroup>
                                </Grid.RenderTransform>
                                <Border Height="196" HorizontalAlignment="Stretch">
                                    <Image
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Bottom"
                                        Source="/Assets/Backgrounds/MantisShrimp.jpg"
                                        Stretch="UniformToFill" />
                                </Border>

                                <Grid VerticalAlignment="Bottom" Background="{StaticResource MainPanelBg}">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>

                                    <TextBlock
                                        Margin="6"
                                        VerticalAlignment="Center"
                                        FontSize="18"
                                        Foreground="White"
                                        Text="Invertebrate" />

                                    <StackPanel
                                        Margin="6,6,6,6"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Center"
                                        Orientation="Horizontal">
                                        <Button
                                            Command="{Binding StartWorkshopCommand}"
                                            CommandParameter="Invert-Workshop"
                                            Content="Workshop"
                                            Style="{StaticResource LauncherButton}" />
                                        <Button
                                            Command="{Binding StartWorkshopCommand}"
                                            CommandParameter="Invert-Quiz"
                                            Content="Quiz"
                                            Style="{StaticResource LauncherButton}" />
                                        <Button
                                            Command="{Binding StartWorkshopCommand}"
                                            CommandParameter="Invert-Exam"
                                            Content="Exam"
                                            Style="{StaticResource LauncherButton}"
                                            Visibility="{Binding IsExamsEnabled, Converter={StaticResource BooleanToVisibility}}" />
                                    </StackPanel>
                                </Grid>
                            </Grid>

                            <Grid
